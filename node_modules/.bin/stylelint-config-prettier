#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/bin/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules:/mnt/d/Vue_trial/Vue-Story/Vue-Story/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/bin/check.js" "$@"
else
  exec node  "$basedir/../.pnpm/stylelint-config-prettier@9_3207690e52d4f908dc5a00c54f366329/node_modules/stylelint-config-prettier/bin/check.js" "$@"
fi
